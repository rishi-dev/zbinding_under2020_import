{
    "_version": "1.12.0",
    "sap.app": {
      "id": "sap.demo2020",
      "type": "application",
      "i18n": "i18n/i18n.properties",
      "title": "{{appTitle}}",
      "description": "{{appDescription}}",
      "applicationVersion": {
        "version": "1.0.0"
      },
      "dataSources": {
        "RemoteData": {
          "uri": "/sap/opu/odata/IWBEP/GWSAMPLE_BASIC/",
          "type": "OData",
          "settings": {
            "odataVersion": "2.0"
          }
        }
      }      
    },
    "sap.ui": {
      "technology": "UI5",
      "deviceTypes": {
          "desktop": true,
          "tablet": true,
          "phone": true
      }
    },
    "sap.ui5": {
      "rootView": {
          "viewName": "sap.demo2020.view.App",
          "type": "XML",
          "async": true,
          "id": "app"
      },
      "dependencies": {
        "minUI5Version": "1.60",
        "libs": {
          "sap.m": {}
        }
      },
      "models": {
        "i18n": {
          "type": "sap.ui.model.resource.ResourceModel",
          "settings": {
            "bundleName": "sap.demo2020.i18n.i18n"
          }
        },
        "bindinglocal": {
          "type": "sap.ui.model.json.JSONModel",
          "uri": "model/sample.json"
          },
        "bindingcal": {
            "dataSource": "RemoteData",
            "preload": true,
            "settings": {
              "defaultBindingMode": "TwoWay"
            }				
          }          	      
      },
      "resources": {
  
      }	
    }
  }